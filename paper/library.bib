@article{Zanelli2015,
  title = {Mineralogical Composition and Particle Size Distribution as a Key to Understand the Technological Properties of {{Ukrainian}} Ball Clays},
  author = {Zanelli, Chiara and Iglesias, Claudio and Domínguez, Eduardo and Gardini, Davide and Raimondo, Mariarosa and Guarini, Guia and Dondi, Michele},
  date = {2015},
  journaltitle = {Applied Clay Science},
  volume = {108},
