@article{Nadeau1984a,
  title = {Interstratified {{XRD}} Characteristics of Physical Mixtures of Elementary Clay Particles},
  author = {Nadeau, P. H. and Tait, J. M. and McHardy, W. J. and Wilson, M. J.},
